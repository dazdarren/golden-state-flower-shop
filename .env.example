# Next.js environment variables
# Copy this file to .env.local for local development

# Required for sitemap and canonical URLs
NEXT_PUBLIC_SITE_URL=https://your-domain.com

# Optional: Florist One credentials for build-time product data fetching
# If not set, placeholder products will be used
FLORISTONE_AFFILIATE_ID=your_affiliate_id
FLORISTONE_API_TOKEN=your_api_token
